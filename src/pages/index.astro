---
import Layout from "../layouts/Layout.astro";
import Header from "../components/Header.astro";
import Booths from "../components/Booths.astro";
import ChicBackdrops from "../components/ChicBackdrops.astro";
import About from "../components/About.astro";
import Partners from "../components/Partners.astro";
import ReviewsWidget from "../components/ReviewsWidget.astro";
import InstantPrints from "../components/InstantPrints.astro";
import BoothFeatures from "../components/BoothFeatures.astro";
import DividerBlock from "../components/DividerBlock.astro";
import BestEventEver from "../components/BestEventEver.astro";
import SearchNoFurther from "../components/SearchNoFurther.astro";
import FunProps from "../components/FunProps.astro";
import Faq from "../components/Faq.astro";
import PrivateGallery from "../components/PrivateGallery.astro";
import BetterImageQuality from "../components/BetterImageQuality.astro";
import GlamStyle from "../components/GlamStyle.astro";
import ReviewSlider from "../components/ReviewSlider.astro";


const cards = [
  {
    iconSrc: "/images/prints.png",
    iconAlt: "Custom Photo Overlay",
    title: "CUSTOM OVERLAY",
    description: "Special messages, event dates or branding",
  },
  {
    iconSrc: "/images/gallery.png",
    iconAlt: "Online Gallery",
    title: "ONLINE GALLERY",
    description:
      "A personalized gallery for guests to download free digital copies",
  },
  {
    iconSrc: "/images/backdrops-props.png",
    iconAlt: "Backdrops / Props",
    title: "BACKDROPS / PROPS",
    description:
      "We have a large selection ready on-hand for you to choose from",
  },
  {
    iconSrc: "/images/screen.png",
    iconAlt: "Welcome Screen",
    title: "CUSTOM TAP TO START SCREEN",
    description: "Supports photos, videos and custom artwork",
  },
];
---

<Layout title="Popstar Photo Booths | Raleigh | Durham | Surrounding Areas">
  <Header />
  <main>
    <BestEventEver />
    <ReviewSlider />
    <SearchNoFurther />
    <Partners />
    <DividerBlock
      headerText="With Our Photo Booth Packages You Are Always Guaranteed"
      headerTextColor=""
      descriptionText=""
      descriptionTextColor=""
      buttonText=""
      buttonTextColor=""
      linkUrl=""
      bgColor="white"
      topMargin="100px"
      bottomMargin="50px"
    />
    <ChicBackdrops />
    <BetterImageQuality />
    <GlamStyle />
    <InstantPrints />
    <FunProps />
    <PrivateGallery />
    <DividerBlock
      headerText="Check Availability"
      headerTextColor="white"
      descriptionText="Popular dates fill up quickly, check your date today!"
      descriptionTextColor="white"
      buttonText="Let's Go!"
      buttonTextColor=""
      linkUrl="https://popstar-photo-booths.checkcherry.com/reservation/set_event?event_category_id=17207"
      bgColor="black"
      topMargin="100px"
      bottomMargin="100px"
    />
    <About />
    <Faq />
  </main>
</Layout>

